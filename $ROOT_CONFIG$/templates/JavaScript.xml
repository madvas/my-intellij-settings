<templateSet group="JavaScript">
  <template name="logconsole" value="console.log();" description="console.log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="con" value="console.log($s$);" shortcut="TAB" description="console.log" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="deb" value="debugger;$END$" description="debugger" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="getclass" value="Ext.getClassName($s$);" description="Ext.getClassName()" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="func" value="function($PARAM$) {&#10;  $END$&#10;}," description="Inserts function expression with comma" toReformat="true" toShortenFQNames="true">
    <variable name="PARAM" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fun" value="function $s$($PARAM$) {&#10;  $END$&#10;}" description="Inserts function expression" toReformat="true" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="funs" value="function($PARAM$) {&#10;  $END$&#10;};" description="Inserts function expression with semicolon" toReformat="true" toShortenFQNames="true">
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="req" value="require('$s$');" description="require" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="don" value="done();" description="done();" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="des" value="describe('$s$', function(){&#10;&#10;});" description="describe" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="it" value="it('should $s$', function() {&#10;&#10;});" description="it" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name=".sav" value=".save(function(err) {&#10;  $s$;&#10;});" description=".save" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="sherr" value="should.exist(err);" description="should.exist(err);" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="shnerr" value="should.not.exist(err);" description="should.not.exist(err);" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="shx" value="should.exist($s$);" description="should.exist();" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="shnx" value="should.not.exist($s$);" description="should.not.exist();" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="befe" value="beforeEach(function(done) {&#10;  $s$;&#10;});" description="beforeEach" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="bef" value="before(function(done) {&#10;  $s$;&#10;});" description="before" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fune" value="function(err) {&#10;  $s$&#10;}" description="function(err)" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="nex" value="next();" description="next();" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="dedone" value="done = function(err, res) {&#10;    done();&#10;};" description="Rewrite done for being able to put breakpoint" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="callp" value="this.callParent(arguments);" description="this.callParent(arguments);" toReformat="false" toShortenFQNames="true">
    <context />
  </template>
  <template name="req;" value="require('$s$');" description="require();" toReformat="false" toShortenFQNames="true">
    <variable name="s" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="conjson" value="console.log(JSON.stringify($END$));" description="console.log json stringify" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JSON" value="false" />
    </context>
  </template>
</templateSet>